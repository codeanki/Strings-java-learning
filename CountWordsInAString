/*
Sample Input1:
Hello World

Sample Output1:
2


Sample Input2:
this is a simple code
Sample Output2:
5
*/

public class Solution{
   public static int CountWords(int n){
   int count =0;
   for(int i=0; i<str.length(); i++){
       if(str.charAt(i) == ' '){
       count++;
       }
    }
    return count+1;
}
}
       
